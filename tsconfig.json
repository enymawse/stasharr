{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "lib": ["ES2016", "DOM"],
    "moduleResolution": "node",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": ["solid-js"]
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/index.tsx"],
  "exclude": ["node_modules"]
}
